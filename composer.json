{
  "name": "HRMVC",
  "type": "project",
  "description": "The Ramtinhr mvc Framework.",
  "keywords": [
    "framework",
    "mvc",
    "ramtinhr"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "symfony/dotenv": "^5.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "filp/whoops": "^2.8",
    "symfony/var-dumper": "^5.1"
  }
}
